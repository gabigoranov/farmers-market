@model ChangeStockViewModel
@{
    ViewData["Title"] = "Down Stock";
}

<form class="form" asp-action="Down" method="post" enctype="multipart/form-data">
    <div class="form-container">
        <input asp-for="Quantity" placeholder="OfferTypeId" aria-required="true" />
        <span asp-validation-for="Quantity" class="text-danger"></span>
    </div>
    <button type="submit">Submit</button>
</form>
